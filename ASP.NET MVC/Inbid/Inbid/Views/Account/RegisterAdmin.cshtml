@model Inbid.Models.RegistrationModel


@{
    ViewBag.Title = "Register Admin";
}

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
 
<link href="@Url.Content("~/Content/Account.css")" rel="stylesheet" type="text/css" />



  

   @if ((Request.IsAuthenticated) && (Roles.IsUserInRole(User.Identity.Name, @"SuperAdmin")))
   {
<h2>Create a New Account For Company Admin</h2>
   
  
<p>
    Passwords are required to be a minimum of 6 characters in length.
</p>

<h2>RegisterAdmin</h2>
    using (Html.BeginForm())
    {
        @Html.ValidationSummary(true, "Account creation was unsuccessful. Please correct the errors and try again.")
        <div>
            @Html.Partial("_RegisterAdmin")
        </div>
        <p>
          <input type="submit" value="@Resources.Resources.Register" />
        </p>
    }
}