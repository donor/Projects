@model Inbid.Models.RegistrationModel

           <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
 
<link href="@Url.Content("~/Content/Account.css")" rel="stylesheet" type="text/css" />


@{
    ViewBag.Title = "Edytuj firme " + @Model.Name;
}

  @if ((Request.IsAuthenticated) && (Roles.IsUserInRole(User.Identity.Name, @"SuperAdmin")))
  {
    <h2>EditCompanyAdmin</h2>       
    <p>
        Passwords are required to be a minimum of 6 characters in length.
   </p>
      

    using (Html.BeginForm())
    {
        @Html.ValidationSummary(true, "Account edit was unsuccessful. Please correct the errors and try again.")
        <div>
            @Html.Partial("_RegisterAdmin")
        </div>
        <p>
          <input type="submit" value="Zapisz" />
        </p>
    }
  }